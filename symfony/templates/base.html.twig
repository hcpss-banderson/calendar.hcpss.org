            <link rel="stylesheet" type='text/css' href="/css/style.css">
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <div class="calendar-page">
            {% block body %}{% endblock %}
        </div>
    </body>
</html>
