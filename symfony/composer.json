{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "johngrogg/ics-parser": "^3.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.5",
        "rlanvin/php-rrule": "^2.3",
        "symfony/asset": "^7",
        "symfony/console": "*",
        "symfony/doctrine-messenger": "^7",
        "symfony/dotenv": "*",
        "symfony/expression-language": "^7",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "^7",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "^7",
        "symfony/intl": "^7",
        "symfony/mailer": "^7",
        "symfony/mime": "^7",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^7",
        "symfony/process": "^7",
        "symfony/property-access": "^7",
        "symfony/property-info": "^7",
        "symfony/runtime": "*",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^7",
        "symfony/string": "^7",
        "symfony/translation": "^7",
        "symfony/twig-bundle": "^7",
        "symfony/validator": "^7",
        "symfony/web-link": "^7",
        "symfony/webpack-encore-bundle": "^2",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7",
            "docker": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^7",
        "symfony/css-selector": "^7",
        "symfony/debug-bundle": "^7",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "^7",
        "symfony/web-profiler-bundle": "*"
    }
}
